@model Student_Management__InfoDevelopers_.Models.students

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>
@if (TempData["ErrorMessage"] != null)
{
    <div class="alert alert-danger">
    <strong>Information:</strong>@TempData["ErrorMessage"]
    </div>
}
<h4>students</h4>
<hr />
<div class="row">
